{
    "name": "honeylex/hlx-security",
    "description": "System account crate for Honeylex.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Foh\\SystemAccount\\": "lib/"
        }
    },
    "scripts": {
        "code-sniffer": "vendor/bin/phpcs --extensions=php --standard=psr2 lib"
    }
}
